{
  "name": "@beauty-salon-backend/infrastructure",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "sideEffects": false,
  "files": ["dist"],
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./database": {
      "import": "./dist/database/index.js",
      "types": "./dist/database/index.d.ts"
    },
    "./src/database/schema": {
      "import": "./dist/database/schema.js",
      "types": "./dist/database/schema.d.ts"
    },
    "./src/test/db-test-helper": {
      "import": "./dist/test/db-test-helper.js",
      "types": "./dist/test/db-test-helper.d.ts"
    },
    "./test/db-test-helper": {
      "import": "./dist/test/db-test-helper.js",
      "types": "./dist/test/db-test-helper.d.ts"
    }
  },
  "scripts": {
    "clean": "rm -rf dist *.tsbuildinfo",
    "build:dev": "NODE_ENV=development rslib build",
    "build:test": "NODE_ENV=test rslib build",
    "build:stg": "NODE_ENV=staging rslib build",
    "build:prod": "NODE_ENV=production rslib build",
    "build": "NODE_ENV=production rslib build",
    "build:watch": "NODE_ENV=development rslib build --watch",
    "dev": "NODE_ENV=development rslib build --watch",
    "dev:tsc": "tsc --build --watch",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "typecheck": "tsc --noEmit",
    "format": "biome format --write .",
    "format:fix": "biome check . --fix --unsafe",
    "lint": "biome lint .",
    "lint:fix": "biome lint . --write",
    "knip": "knip --include dependencies,optionalPeerDependencies,unlisted,binaries,unresolved",
    "package:check": "taze -r"
  },
  "dependencies": {
    "@beauty-salon-backend/domain": "workspace:*",
    "@beauty-salon-backend/config": "workspace:*",
    "@beauty-salon-backend/types": "workspace:*",
    "drizzle-orm": "^0.44.2",
    "postgres": "^3.4.7",
    "dotenv": "^16.5.0",
    "nodemailer": "^6.9.19",
    "mailgun.js": "^10.3.2",
    "form-data": "^4.0.1",
    "date-fns": "^4.1.0",
    "@aws-sdk/client-s3": "^3.709.0",
    "@aws-sdk/s3-request-presigner": "^3.709.0"
  },
  "devDependencies": {
    "@beauty-salon-backend/migration": "workspace:*",
    "@beauty-salon-shared/config": "workspace:*",
    "@beauty-salon-backend/test-utils": "workspace:*",
    "@biomejs/biome": "^1.9.4",
    "@rslib/core": "^0.10.5",
    "@testcontainers/postgresql": "^11.2.1",
    "@types/node": "^24.0.13",
    "@types/nodemailer": "^6.4.17",
    "drizzle-kit": "^0.31.4",
    "knip": "^5.61.3",
    "taze": "^19.1.0",
    "testcontainers": "^11.2.1",
    "typescript": "^5.8.3",
    "vite": "^7.0.4",
    "vite-plugin-dts": "^4.5.4",
    "vitest": "^3.2.4"
  }
}

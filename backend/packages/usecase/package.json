{
  "name": "@beauty-salon-backend/usecase",
  "version": "1.0.0",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "scripts": {
    "clean": "rm -rf dist *.tsbuildinfo",
    "build:dev": "NODE_ENV=development rslib build",
    "build:test": "NODE_ENV=test rslib build",
    "build:stg": "NODE_ENV=staging rslib build",
    "build:prod": "NODE_ENV=production rslib build",
    "build": "NODE_ENV=production rslib build",
    "build:watch": "NODE_ENV=development rslib build --watch",
    "dev": "NODE_ENV=development rslib build --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "typecheck": "tsc --noEmit",
    "lint": "biome check --write .",
    "lint:check": "biome check ."
  },
  "dependencies": {
    "@beauty-salon-backend/domain": "workspace:*",
    "@beauty-salon-backend/types": "workspace:*",
    "bcrypt": "^6.0.0",
    "qrcode": "^1.5.4",
    "speakeasy": "^2.0.0",
    "ts-pattern": "^5.5.0",
    "uuid": "^11.0.6"
  },
  "devDependencies": {
    "@rslib/core": "^0.10.5",
    "@types/bcrypt": "^5.0.2",
    "@types/node": "^22.12.1",
    "@types/qrcode": "^1.5.5",
    "@types/speakeasy": "^2.0.10",
    "@types/uuid": "^10.0.0",
    "vitest": "^3.2.4"
  }
}

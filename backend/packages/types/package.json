{
  "name": "@beauty-salon-backend/types",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "sideEffects": false,
  "files": ["dist"],
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./api": {
      "import": "./dist/api.js",
      "types": "./dist/api.d.ts"
    },
    "./generated": {
      "import": "./dist/generated/index.js",
      "types": "./dist/generated/index.d.ts"
    }
  },
  "scripts": {
    "clean": "rm -rf dist *.tsbuildinfo",
    "build:dev": "NODE_ENV=development rslib build",
    "build:test": "NODE_ENV=test rslib build",
    "build:stg": "NODE_ENV=staging rslib build",
    "build:prod": "NODE_ENV=production rslib build",
    "build": "NODE_ENV=production rslib build",
    "build:watch": "NODE_ENV=development rslib build --watch",
    "dev": "NODE_ENV=development rslib build --watch",
    "dev:tsc": "tsc --build --watch",
    "typecheck": "tsc --build",
    "format": "biome format --write .",
    "format:fix": "biome check . --fix --unsafe",
    "lint": "biome lint .",
    "lint:fix": "biome lint . --write",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "knip": "knip --include dependencies,optionalPeerDependencies,unlisted,binaries,unresolved",
    "package:check": "taze -r",
    "generate": "tsx scripts/generate-types.ts",
    "generate:types": "tsx scripts/generate-types.ts"
  },
  "dependencies": {
    "uuid": "^11.1.0",
    "zod": "3.25.76"
  },
  "devDependencies": {
    "@beauty-salon-shared/config": "workspace:*",
    "@biomejs/biome": "^1.9.4",
    "@rslib/core": "^0.10.5",
    "@types/node": "^24.0.13",
    "@types/uuid": "^10.0.0",
    "knip": "^5.61.3",
    "openapi-typescript": "^7.8.0",
    "taze": "^19.1.0",
    "tsx": "^4.20.3",
    "typescript": "^5.8.3",
    "vite": "^7.0.4",
    "vite-plugin-dts": "^4.5.4",
    "yaml": "^2.3.4"
  }
}

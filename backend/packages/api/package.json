{
  "name": "@beauty-salon-backend/api",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "sideEffects": false,
  "files": ["dist"],
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "clean": "rm -rf dist *.tsbuildinfo",
    "build:dev": "NODE_ENV=development rslib build",
    "build:test": "NODE_ENV=test rslib build",
    "build:stg": "NODE_ENV=staging rslib build",
    "build:prod": "NODE_ENV=production rslib build",
    "build": "NODE_ENV=production rslib build",
    "build:watch": "NODE_ENV=development rslib build --watch",
    "dev": "NODE_ENV=development rslib build --watch",
    "dev:tsc": "tsc --build --watch",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:integration": "vitest run --config vitest.integration.config.ts",
    "typecheck": "tsc --build",
    "format": "biome format --write .",
    "format:fix": "biome check . --fix --unsafe",
    "lint": "biome lint .",
    "lint:fix": "biome lint . --write",
    "knip": "knip --include dependencies,optionalPeerDependencies,unlisted,binaries,unresolved",
    "package:check": "taze -r"
  },
  "dependencies": {
    "@backend/usecase": "workspace:*",
    "@beauty-salon-backend/domain": "workspace:*",
    "@beauty-salon-backend/infrastructure": "workspace:*",
    "@beauty-salon-backend/types": "workspace:*",
    "bcrypt": "^6.0.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "drizzle-orm": "^0.44.2",
    "express": "^4.21.0",
    "helmet": "^8.0.0",
    "jsonwebtoken": "^9.0.2",
    "ms": "^2.1.3",
    "pino": "^9.5.0",
    "pino-http": "^10.3.0",
    "pino-pretty": "^13.0.0",
    "postgres": "^3.4.7",
    "zod": "3.25.76"
  },
  "devDependencies": {
    "@beauty-salon-shared/config": "workspace:*",
    "@biomejs/biome": "^1.9.4",
    "@rslib/core": "^0.10.5",
    "@testcontainers/postgresql": "^11.2.1",
    "@types/bcrypt": "^5.0.2",
    "@types/compression": "^1.7.5",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/ms": "^2.1.0",
    "@types/node": "^24.0.13",
    "@types/supertest": "^6.0.2",
    "drizzle-kit": "^0.31.4",
    "knip": "^5.61.3",
    "supertest": "^7.0.0",
    "taze": "^19.1.0",
    "testcontainers": "^11.2.1",
    "typescript": "^5.8.3",
    "vite": "^7.0.4",
    "vite-plugin-dts": "^4.5.4",
    "vitest": "^3.2.4"
  }
}

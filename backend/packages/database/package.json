{
  "name": "@beauty-salon-backend/database",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    }
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "clean": "rm -rf dist *.tsbuildinfo",
    "build:localhost": "NODE_ENV=localhost rslib build && tsc --emitDeclarationOnly",
    "build:dev": "NODE_ENV=development rslib build && tsc --emitDeclarationOnly",
    "build:test": "NODE_ENV=test rslib build && tsc --emitDeclarationOnly",
    "build:stg": "NODE_ENV=staging rslib build && tsc --emitDeclarationOnly",
    "build:prod": "NODE_ENV=production rslib build && tsc --emitDeclarationOnly",
    "build:watch": "NODE_ENV=development rslib build --watch",
    "dev": "NODE_ENV=development rslib build --watch",
    "dev:tsc": "tsc --build --watch",
    "typecheck": "tsc --noEmit",
    "format": "biome format --write .",
    "format:fix": "biome check . --fix --unsafe",
    "lint": "biome lint .",
    "lint:fix": "biome lint . --write",
    "db:migrate": "tsx src/scripts/migrate.ts",
    "db:reset": "tsx src/scripts/reset-db.ts",
    "db:setup": "tsx src/scripts/setup-db.ts",
    "db:introspect": "tsx src/scripts/introspect.ts",
    "db:generate-sql": "tsx src/scripts/generate-setup-sql.ts",
    "db:fresh": "pnpm db:reset && pnpm db:setup",
    "db:seed": "tsx src/scripts/populate-seed.ts",
    "db:truncate": "tsx src/scripts/truncate-seed.ts"
  },
  "dependencies": {
    "@beauty-salon-backend/config": "workspace:*",
    "@beauty-salon-backend/utility": "workspace:*",
    "drizzle-orm": "^0.44.5",
    "postgres": "^3.4.7"
  },
  "devDependencies": {
    "@beauty-salon-shared/config": "workspace:*",
    "@biomejs/biome": "^2.2.4",
    "@rslib/core": "^0.14.0",
    "@types/node": "^24.5.2",
    "dotenv": "^17.2.2",
    "drizzle-kit": "^0.31.4",
    "tsx": "^4.20.6",
    "typescript": "^5.9.2"
  }
}

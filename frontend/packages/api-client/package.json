{
  "name": "@beauty-salon-frontend/api-client",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "scripts": {
    "dev": "tsc --watch",
    "build": "tsc",
    "build:rslib": "rslib build",
    "clean": "rm -rf dist *.tsbuildinfo",
    "typecheck": "tsc --noEmit",
    "format": "biome format --write .",
    "format:fix": "biome check . --fix --unsafe",
    "generate": "orval"
  },
  "dependencies": {
    "@beauty-salon-frontend/io": "workspace:*",
    "@beauty-salon-frontend/types": "workspace:*",
    "@biomejs/biome": "^2.2.5",
    "@rslib/core": "^0.14.0",
    "@tanstack/react-query": "^5.90.2"
  },
  "devDependencies": {
    "@beauty-salon-shared/config": "workspace:*",
    "@biomejs/biome": "^2.2.5",
    "@types/node": "^24.5.2",
    "orval": "^7.11.2"
  }
}

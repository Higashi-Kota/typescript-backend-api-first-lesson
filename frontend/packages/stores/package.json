{
  "dependencies": {
    "@ultimate/vault": "^0.0.2"
  },
  "devDependencies": {
    "@beauty-salon-shared/config": "workspace:*",
    "@biomejs/biome": "^2.2.4",
    "@rslib/core": "^0.14.0",
    "@types/node": "^24.6.2",
    "typescript": "^5.9.2",
    "vitest": "^3.2.4"
  },
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "name": "@beauty-salon-frontend/stores",
  "sideEffects": false,
  "private": true,
  "scripts": {
    "clean": "rm -rf dist *.tsbuildinfo",
    "build:dev": "NODE_ENV=development rslib build",
    "build:test": "NODE_ENV=test rslib build",
    "build:stg": "NODE_ENV=staging rslib build",
    "build:prod": "NODE_ENV=production rslib build",
    "build:watch": "NODE_ENV=development rslib build --watch",
    "typecheck": "tsc --build",
    "format": "biome format --write .",
    "format:fix": "biome check . --fix --unsafe",
    "dev": "NODE_ENV=development rslib build --watch",
    "dev:tsc": "tsc --build --watch",
    "test": "vitest"
  },
  "type": "module",
  "types": "./dist/index.d.ts",
  "version": "0.0.0"
}

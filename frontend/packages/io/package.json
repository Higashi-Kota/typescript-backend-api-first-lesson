{
  "dependencies": {
    "@beauty-salon-frontend/config": "workspace:*",
    "@beauty-salon-frontend/stores": "workspace:*",
    "@beauty-salon-frontend/types": "workspace:*",
    "@beauty-salon-frontend/utils": "workspace:*",
    "@tanstack/react-query": "^5.90.2",
    "neverthrow": "^8.2.0"
  },
  "devDependencies": {
    "@beauty-salon-shared/config": "workspace:*",
    "@biomejs/biome": "^2.2.4",
    "@rslib/core": "^0.14.0",
    "@types/node": "^24.7.2",
    "typescript": "^5.9.2",
    "vite": "^7.1.7",
    "vite-plugin-dts": "^4.5.4"
  },
  "exports": {
    ".": {
      "import": "./dist/index.js",

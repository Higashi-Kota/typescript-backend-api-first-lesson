{
  "dependencies": {
    "@beauty-salon-frontend/config": "workspace:*",
    "@beauty-salon-frontend/stores": "workspace:*",
    "@beauty-salon-frontend/types": "workspace:*",
    "@beauty-salon-frontend/utils": "workspace:*",
    "@tanstack/react-query": "^5.83.0",
    "neverthrow": "^8.2.0"
  },
  "devDependencies": {
    "@beauty-salon-shared/config": "workspace:*",
    "@biomejs/biome": "^1.9.4",
    "@rslib/core": "^0.10.5",
    "@types/node": "^24.0.13",
    "@vitejs/plugin-react-swc": "^3.10.2",
    "knip": "^5.61.3",
    "taze": "^19.1.0",
    "typescript": "^5.8.3",
    "vite": "^7.0.4",
    "vite-plugin-dts": "^4.5.4"
  },
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./domains": {
      "import": "./dist/domains/index.js",
      "types": "./dist/domains/index.d.ts"
    },
    "./factory": {
      "import": "./dist/factory/index.js",
      "types": "./dist/factory/index.d.ts"
    },
    "./hocs": {
      "import": "./dist/hocs/index.js",
      "types": "./dist/hocs/index.d.ts"
    },
    "./hooks": {
      "import": "./dist/hooks/index.js",
      "types": "./dist/hooks/index.d.ts"
    },
    "./manager": {
      "import": "./dist/manager/index.js",
      "types": "./dist/manager/index.d.ts"
    },
    "./repositories": {
      "import": "./dist/repositories/index.js",
      "types": "./dist/repositories/index.d.ts"
    },
    "./types": {
      "import": "./dist/types/index.js",
      "types": "./dist/types/index.d.ts"
    }
  },
  "files": ["dist"],
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "name": "@beauty-salon-frontend/io",
  "sideEffects": false,
  "private": true,
  "scripts": {
    "clean": "rm -rf dist *.tsbuildinfo",
    "build": "NODE_ENV=production rslib build",
    "build:dev": "NODE_ENV=development rslib build",
    "build:test": "NODE_ENV=test rslib build",
    "build:stg": "NODE_ENV=staging rslib build",
    "build:prod": "NODE_ENV=production rslib build",
    "build:watch": "NODE_ENV=development rslib build --watch",
    "package:check": "taze",
    "knip": "knip --include dependencies,optionalPeerDependencies,unlisted,binaries,unresolved",
    "typecheck": "tsc --build",
    "format": "biome format --write .",
    "format:fix": "biome check . --fix --unsafe",
    "dev": "NODE_ENV=development rslib build --watch",
    "dev:tsc": "tsc --build --watch"
  },
  "type": "module",
  "types": "./dist/index.d.ts",
  "version": "0.0.0"
}

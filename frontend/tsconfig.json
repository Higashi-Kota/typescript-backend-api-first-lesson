{
  "extends": "@beauty-salon-shared/config/frontend.json",
  "compilerOptions": {
    "composite": false,
    "noEmit": true,
    "rootDir": ".",
    "baseUrl": ".",
    "types": [],
    "paths": {
      "@beauty-salon-frontend/ui": ["./packages/ui/src"],
      "@beauty-salon-frontend/ui/*": ["./packages/ui/src/*"],
      "@beauty-salon-frontend/types": ["./packages/types/src"],
      "@beauty-salon-frontend/types/*": ["./packages/types/src/*"],
      "@beauty-salon-frontend/api-client": ["./packages/api-client/src"],
      "@beauty-salon-frontend/api-client/*": ["./packages/api-client/src/*"],
      "@beauty-salon-frontend/utils": ["./packages/utils/src"],
      "@beauty-salon-frontend/utils/*": ["./packages/utils/src/*"],
      "@beauty-salon-frontend/assets": ["./packages/assets/src"],
      "@beauty-salon-frontend/assets/*": ["./packages/assets/src/*"],
      "@beauty-salon-frontend/config": ["./packages/config/src"],
      "@beauty-salon-frontend/config/*": ["./packages/config/src/*"],
      "@beauty-salon-frontend/io": ["./packages/io/src"],
      "@beauty-salon-frontend/io/*": ["./packages/io/src/*"],
      "@beauty-salon-frontend/message": ["./packages/message/src"],
      "@beauty-salon-frontend/message/*": ["./packages/message/src/*"],
      "@beauty-salon-frontend/stores": ["./packages/stores/src"],
      "@beauty-salon-frontend/stores/*": ["./packages/stores/src/*"],
      "@beauty-salon-frontend/validations": ["./packages/validations/src"],
      "@beauty-salon-frontend/validations/*": ["./packages/validations/src/*"]
    }
  },
  "references": [
    { "path": "./packages/types" },
    { "path": "./packages/ui" },
    { "path": "./packages/api-client" },
    { "path": "./packages/utils" },
    { "path": "./packages/assets" },
    { "path": "./packages/config" },
    { "path": "./packages/io" },
    { "path": "./packages/message" },
    { "path": "./packages/stores" },
    { "path": "./packages/validations" }
  ]
}
